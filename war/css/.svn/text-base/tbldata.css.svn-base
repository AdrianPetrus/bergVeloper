.tbl-data {
	border-bottom: 0px solid #ddd;

}
.tbl-data table, table.ng-table {
	width: 100%;
	border-collapse: collapse;
}
.tbl-data table tbody tr, table.ng-table tbody tr {
	cursor: pointer;
}
.tbl-data table tr th, table.ng-table tr th {
	border-bottom-width: 2px;
	border-bottom-style: solid;
	border-bottom-color: #DDD;
	font-style: italic;
	font-weight: normal;
	color: #098E21;
	font-size: 11px;
	line-height: 13px;
	text-align: left;
	vertical-align: top;
	padding-top: 1px;
	padding-right: 4px;
	padding-bottom: 1px;
	padding-left: 4px;
}
.tbl-data table tr td, table.ng-table tr td {
	padding-top: 2px;
	padding-right: 4px;
	padding-bottom: 2px;
	padding-left: 4px;
	border-bottom: 1px solid #eeeeee;
	vertical-align: top;
}
.tbl-data table tr.selected td, table.ng-table tr.selected td {
	background-color: #EEE;
	background-color: rgba(141, 192, 219, 0.25);
    text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75);
}
.tbl-data table tr.selected td.last, table.ng-table tr.selected td.last {
	background-image: url(../images/arow-right.png);
	background-repeat: no-repeat;
	background-position: center center;
	padding: 0px;
	width: 16px;
}
.tbl-data table tr.edit-mode, table.ng-table tr.edit-mode {
	background-color: #E3E3E3;
}
.tbl-data table tr.edit-mode td, table.ng-table tr.edit-mode td {
	padding: 15px;
	border-right-width: 1px;
	border-left-width: 1px;
	border-right-style: solid;
	border-left-style: solid;
	border-right-color: #CCC;
	border-left-color: #CCC;
}
.tbl-data table tr.edit-mode .position-form, table.ng-table tr.edit-mode .position-form {
	padding: 0px;
}
.tbl-data table tr.confirmed td, table.ng-table tr.confirmed td {
	background-color: #CFC;
}
.tbl-data table tr.confirmed td.last, table.ng-table tr.confirmed td.last {
	background-image: url(../images/icon-confirmation.png);
	background-repeat: no-repeat;
	background-position: center center;
	width: 16px;
	padding: 0px;
}
.tbl-data table tr td.center, .tbl-data table tr th.center, table.ng-table tr td.center, table.ng-table tr th.center{
	text-align: center;
}
.tbl-data table tr td.nowrap, .tbl-data table tr th.nowrap, table.ng-table tr td.nowrap, table.ng-table tr th.nowrap {
	white-space: nowrap;
}
.tbl-data table tr td.red, table.ng-table tr td.red {
	color: #FF0004;
}

table.ng-table tbody td:first-child {
  /*border-right: 1px solid #ccc;*/
  background-color: #E3E3E3;
}

table.ng-table tbody td:not(:first-child) {
