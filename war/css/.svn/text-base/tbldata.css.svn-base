.over   {
	background-color: #e5eff5;
}
.pagination {
	position: relative;
	padding-left: 50%;
	height: 20px;
	padding-top: 5px;
}

.pagination-inside {
	position: absolute;
	left: 50%;
}

.pagination ul {
	display: block;
	position: relative;
	left: -50%;
	list-style-image: none;
	list-style-type: none;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	padding-top: 0px;
	padding-right: 0px;
	padding-bottom: 0px;
	padding-left: 0px;
}
.pagination ul li {
	display: inline;
}
.pagination ul li a {
	display: block;
	float: left;
	padding-top: 2px;
	padding-right: 1px;
	padding-bottom: 2px;
	padding-left: 1px;
	text-align: center;
	width: 16px;
	border: 1px solid #DCDCDC;
	margin-left: 2px;
	margin-right: 2px;
	font-size: 11px;
	line-height: 12px;
}
.pagination ul li a:hover {
	border-color: #069;
}
.pagination ul li.previous a, .pagination ul li.next a  {
	border-color: #FFFFFF;
}
.pagination ul li.selected a {
	background-color: #069;
	color: #FFFFFF;
}

.ng-table-pager {
	overflow: auto;	
}
.ng-table-counts {
	float: right;
}
.ng-table-counts .btn {
	font-size: 11px;
	line-height: 12px;
	margin: 0px;
	padding: 0px;
}
ul.pagination {
	position: relative;
	padding-left: 0px;
	height: 19px;
	display: block;
	position: relative;
	list-style-image: none;
	list-style-type: none;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	padding-top: 1px;
	padding-right: 0px;
	padding-bottom: 0px;
	padding-left: 0px;
	float: left;
}
ul.pagination li {
	display: inline;
}
ul.pagination li a {
	display: block;
	float: left;
	padding-top: 2px;
	padding-right: 1px;
	padding-bottom: 2px;
	padding-left: 1px;
	text-align: center;
	width: 16px;
	border: 1px solid #AAAAAA;
	margin-left: 2px;
	margin-right: 2px;
	font-size: 11px;
	line-height: 12px;
}
ul.pagination li a:hover {
	border-color: #069;
}
ul.pagination li.disabled a {
	border: 1px solid #DCDCDC;
}

ul.pagination li.previous a, ul.pagination li.next a  {
	border-color: #FFFFFF;
}
ul.pagination li.selected a {
	background-color: #069;
	color: #FFFFFF;
}
